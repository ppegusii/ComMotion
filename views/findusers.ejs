<!DOCTYPE html>
<html>

<head>
<meta charset=”utf-8”> 

<title><%= title %></title>
<meta name="viewport" content="width=device-width, initial-scale=1">

<link rel = "stylesheet" type ="text/css" href = "stylesheets/bootstrap/css/bootstrap.css" >

<script src="../javascripts/jquery-1.11.0.min.js"></script>

</head>

<body>

<% include fragments/header %>

	<script>
	$( "#findusers" ).addClass("active");
	</script>

<div class="container">

<div class="row">

		<div class="col-md-4 col-sm-4 col-xs-4">

			<% include fragments/usersearchpanel %>			

			</div>

	<div class="col-md-8 col-sm-8 col-xs-8">


		<!--Populates a 5x2 grid of popular exercises -->
	
	<h3>Popular Users</h3>
	<hr>

	<% for (var i=0; i<2; i++){ %>

	<div style="margin-top:20px" class="row">
	
	<div class="col-md-2 col-sm-2 col-xs-2 col-md-offset-1 col-sm-offset-1 col-xs-offset-1">
		<center>
			<media>
				<img src="images/generic_person.jpg" width="80" height="80"></img>
			</media>
			<h4>User <%= i*5+1 %></h4>
		</center>
	
	</div>
	<% for (var j=0; j<4; j++){ %>
		<div class="col-md-2 col-sm-2 col-xs-2">
		<center>
			<media>
				<img src="images/generic_person.jpg" width="80" height="80"></img>
			</media>
			<h4>User <%= i*5+j+2 %></h4>
		</center>

		</div>
	<% } %>

	</div>
	
	<% } %>

	<!--Populates a 5x2 grid of popular exercises -->
	
	<h3>New Users</h3>
	<hr>

	<% for (var i=0; i<2; i++){ %>

	<div style="margin-top:20px" class="row">
	
	<div class="col-md-2 col-sm-2 col-xs-2 col-md-offset-1 col-sm-offset-1 col-xs-offset-1">
		<center>
			<media>
				<img src="images/generic_person.jpg" width="80" height="80"></img>
			</media>
			<h4>User <%= i*5+1 %></h4>
		</center>
	
	</div>
	<% for (var j=0; j<4; j++){ %>
		<div class="col-md-2 col-sm-2 col-xs-2">
		<center>
			<media>
				<img src="images/generic_person.jpg" width="80" height="80"></img>
			</media>
			<h4>User <%= i*5+j+2 %></h4>
		</center>

		</div>
	<% } %>

	</div>
	
	<% } %>


		</div>

	</div>
		
</div>

<% include fragments/footer %>

<script src = "javascripts/bootstrap.js"></script>


</body>


</html>

